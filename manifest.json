{
  "name": "Tabata Timer",
  "short_name": "Tabata",
  "description": "A customizable Tabata timer for high-intensity interval training workouts",
  "start_url": "./",
  "scope": "./",
  "display": "standalone",
  "orientation": "portrait-primary",
  "theme_color": "#007aff",
  "background_color": "#f4f7f9",
  "categories": ["fitness", "health", "sports"],
  "lang": "en",
  "dir": "ltr",
  "icons": [
    {
      "src": "./icons/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any maskable"
    },
    {
      "src": "./icons/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any maskable"
    },
    {
      "src": "./icons/favicon.ico",
      "sizes": "16x16 32x32 48x48",
      "type": "image/x-icon"
    }
  ],
  "file_handlers": [
    {
      "action": "./",
      "accept": {
        "application/json": [".json"]
      },
      "launch_type": "single-client"
    }
  ],
  "screenshots": [
    {
      "src": "./icons/screenshot-wide.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Tabata Timer main interface"
    }
  ],
  "share_target": {
    "action": "./",
    "method": "GET",
    "params": {
      "files": [
        {
          "name": "workout",
          "accept": ["application/json", ".json"]
        }
      ]
    }
  }
}